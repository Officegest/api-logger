{
  "name": "officegest/api-logger",
  "description": "Monitoring and log your APIs",
  "license": "MIT",
  "homepage": "https://officegest.com/",
  "type": "library",
  "version": "1.2.1",
  "keywords": [
    "api",
    "debuging",
    "laravel",
    "monitoring",
    "officegest",
    "guisoft"
  ],
  "authors": [
    {
      "role": "Developer",
      "name": "Stephane Soares",
      "email": "stephane.soares@guisoft.net",
      "homepage": "https://guisoft.net/"
    }
  ],
  "autoload": {
    "psr-4": {
      "OfficegestApiLogger\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "elasticsearch/elasticsearch": "*"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^7.7.0",
    "laravel/octane": "^1.5.6",
    "laravel/pint": "^1.10.3",
    "orchestra/testbench": "^7.0 || ^8.5.9",
    "phpstan/phpstan": "^1.10.25"
  },
  "extra": {
    "laravel": {
      "providers": [
        "OfficegestApiLogger\\OfficegestApiLoggerServiceProvider"
      ]
    }
  },
  "scripts": {
    "pint": [
      "./vendor/bin/pint"
    ],
    "stan": [
      "./vendor/bin/phpstan analyse"
    ],
    "test": [
      "./vendor/bin/phpunit"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true,
      "pestphp/pest-plugin": true
    }
  }
}
